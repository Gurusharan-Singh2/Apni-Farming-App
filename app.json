{
  "expo": {
    "name": "ApniFarming",
    "slug": "apni-farming",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "apnifarming",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "icon": "./assets/images/logo-final.png",
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gurusharan.apnifarming"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
         "image":"./assets/images/splashscreen_logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cd9e30d2-7ad0-43c4-9b47-1a53a9487eed"
      }
    },
    "owner": "gurusharan"
  }
}
